---
layout: null
---
{
  {% for post in site.posts %}

    "{{ post.url | slugify }}": {
      "title": "{{ post.title | json}}",
      "content": "{{ post.excerpt | strip_newlines | json }}",
      "url": "{{ post.url | xml_escape }}",
      "author": "{{ post.author | json }}",
      "category": "{{ post.category | json }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}
